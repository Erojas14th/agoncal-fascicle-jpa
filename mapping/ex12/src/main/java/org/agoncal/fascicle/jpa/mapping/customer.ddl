CREATE TABLE CUSTOMER
(
  ID          BIGINT NOT NULL,
  EMAIL       VARCHAR,
  FIRSTNAME   VARCHAR,
  LASTNAME    VARCHAR,
  PHONENUMBER VARCHAR,
  PRIMARY KEY (ID)
)
CREATE TABLE Customer_ADDRESSES
(
  CITY        VARCHAR,
  COUNTRY     VARCHAR,
  STATE       VARCHAR,
  STREET1     VARCHAR,
  STREET2     VARCHAR,
  ZIPCODE     VARCHAR,
  Customer_ID BIGINT
)
